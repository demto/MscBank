@model MScBank.Models.ApplicationUser
@{
    ViewBag.Title = "MyDetails";
}

<h2>My Details</h2>
<p>Name : @Model.FullName</p>
<p>Date of Birth : @Model.DateOfBirth</p>
<p>Address : @Model.Address</p>
<p>Phone number : @Model.PhoneNumber</p>
<p>Email : @Model.Email</p>

<h2 class="text-warning">Should you wish to change any of the above details please visit your local branch with a photo ID</h2>

